{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryanalitics"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/tweet2')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ParquetSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"wildcardFileName": "*.parquet",
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "ParquetTweet",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "tweet2",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/tweet2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/tweet2')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlTwitterMagister",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "id_str",
						"type": "varchar"
					},
					{
						"name": "created_at",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "text",
						"type": "nvarchar"
					},
					{
						"name": "truncated",
						"type": "bit"
					},
					{
						"name": "in_reply_to_status_id",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "in_reply_to_status_id_str",
						"type": "nvarchar"
					},
					{
						"name": "in_reply_to_user_id",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "in_reply_to_user_id_str",
						"type": "nvarchar"
					},
					{
						"name": "in_reply_to_screen_name",
						"type": "nvarchar"
					},
					{
						"name": "quoted_status_id",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "is_quote_status",
						"type": "bit"
					},
					{
						"name": "quote_count",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "reply_count",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "retweet_count",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "favorite_count",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "favorited",
						"type": "bit"
					},
					{
						"name": "retweeted",
						"type": "bit"
					},
					{
						"name": "lang",
						"type": "nvarchar"
					},
					{
						"name": "Activo",
						"type": "bit"
					},
					{
						"name": "fechacreacion",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "procesado",
						"type": "bit"
					},
					{
						"name": "id_user",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "contadorTweet",
						"type": "int",
						"precision": 10
					},
					{
						"name": "diaMes",
						"type": "int",
						"precision": 10
					},
					{
						"name": "diaSemana",
						"type": "int",
						"precision": 10
					},
					{
						"name": "diaAgno",
						"type": "int",
						"precision": 10
					},
					{
						"name": "nombreDia",
						"type": "varchar"
					},
					{
						"name": "numeroSemanaAgno",
						"type": "int",
						"precision": 10
					},
					{
						"name": "mes",
						"type": "int",
						"precision": 10
					},
					{
						"name": "agno",
						"type": "int",
						"precision": 10
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "Tweet2"
				}
			},
			"dependsOn": []
		}
	]
}