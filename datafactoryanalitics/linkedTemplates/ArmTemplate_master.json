{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryanalitics"
		},
		"AzureSqlTwitterMagister_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlTwitterMagister'"
		},
		"GoogleCloudStorage1_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'GoogleCloudStorage1'"
		},
		"storagegen2multiuso_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storagegen2multiuso'"
		},
		"GoogleCloudStorage1_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOGASSI2M7OBQHAUDCXHROC"
		},
		"storagegen2multiuso_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagegen2multiuso.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlTwitterMagister_connectionString": {
						"value": "[parameters('AzureSqlTwitterMagister_connectionString')]"
					},
					"GoogleCloudStorage1_secretAccessKey": {
						"value": "[parameters('GoogleCloudStorage1_secretAccessKey')]"
					},
					"storagegen2multiuso_accountKey": {
						"value": "[parameters('storagegen2multiuso_accountKey')]"
					},
					"GoogleCloudStorage1_properties_typeProperties_accessKeyId": {
						"value": "[parameters('GoogleCloudStorage1_properties_typeProperties_accessKeyId')]"
					},
					"storagegen2multiuso_properties_typeProperties_url": {
						"value": "[parameters('storagegen2multiuso_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}